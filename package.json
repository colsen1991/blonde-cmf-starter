{
  "name": "blonde-cmf-starter",
  "version": "1.0.0",
  "description": "Starter kit for apps that use Blonde CMF",
  "scripts": {
    "test": "export NODE_ENV=test",
    "build": "export NODE_ENV=production && npm run clean && webpack --config webpack.config.prod.js",
    "server": "export NODE_ENV=development && nodemon server/server.js",
    "frontend": "export NODE_ENV=development && webpack-dev-server --config webpack.config.dev --content-base web/",
    "dev": "export NODE_ENV=development && pm2 start webpack-dev-server -- --config webpack.config.dev --content-base web/ --hot && pm2 start nodemon -- server/server.js",
    "prod": "export NODE_ENV=production && pm2 start server/server.js",
    "lint": "eslint server/ web/",
    "clean": "rm -rf build/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/colsen1991/blonde-cmf-starter.git"
  },
  "keywords": [
    "blonde-cmf",
    "cmf",
    "html",
    "css",
    "javascript",
    "js",
    "vue",
    "pouchdb",
    "node",
    "express",
    "webpack",
    "babel"
  ],
  "author": "Christer Olsen <colsen1991@gmail.com> (https://christerolsen.no)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/colsen1991/blonde-cmf-starter/issues"
  },
  "homepage": "https://github.com/colsen1991/blonde-cmf-starter#readme",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.27.3",
    "eslint": "^3.19.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-vue": "^2.0.1",
    "json-loader": "^0.5.4",
    "nodemon": "^1.11.0",
    "pm2": "^2.4.6",
    "vue-loader": "^11.1.4",
    "vue-template-compiler": "^2.2.4",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "express": "^5.0.0-alpha.5",
    "promise-polyfill": "^6.0.2",
    "vue": "^2.3.3",
    "vue-loader": "^11.3.4",
    "vue-router": "^2.5.3",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.1.2",
    "webpack": "^2.5.1",
    "whatwg-fetch": "^2.0.3"
  }
}
